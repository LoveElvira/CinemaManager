    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="@dimen/size_60"
        android:gravity="center"
        android:orientation="horizontal">

        <android.support.v7.widget.RecyclerView
            android:id="@+id/content_cinema__listview"
            style="@style/WrapWrap" />
    </LinearLayout>

    <LinearLayout
        android:id="@+id/content_cinema__address_layout"
        style="@style/MatchWrap"
        android:orientation="vertical"
        android:visibility="gone">

        <LinearLayout
            style="@style/MatchWrap"
            android:background="@color/white"
            android:gravity="center"
            android:orientation="horizontal"
            android:padding="@dimen/size_10">

            <ImageView
                android:id="@+id/content_cinema__address_image"
                android:layout_width="@dimen/size_30"
                android:layout_height="@dimen/size_30"
                android:layout_marginRight="@dimen/size_10" />

            <TextView
                android:id="@+id/content_cinema__address"
                android:layout_width="0dp"
                android:layout_height="wrap_content"
                android:layout_weight="1"
                android:textColor="@color/black"
                android:textSize="@dimen/text_12" />
        </LinearLayout>
    </LinearLayout>

    <View
        style="@style/MatchOne"
        android:background="@color/gray_e4e4e4" />

    <LinearLayout
        android:id="@+id/content_cinema__time_and_tel_layout"
        style="@style/MatchWrap"
        android:orientation="vertical"
        android:visibility="gone" />

    <LinearLayout
        android:id="@+id/content_cinema__traffic_layout"
        style="@style/MatchWrap"
        android:layout_marginTop="@dimen/size_10"
        android:orientation="vertical"
        android:visibility="gone" />

    <LinearLayout
        android:id="@+id/content_cinema__feature_layout"
        style="@style/MatchWrap"
        android:layout_marginTop="@dimen/size_10"
        android:orientation="vertical"
        android:visibility="gone" />

